{
  "name": "@billyfree/eventstore-client",
  "version": "0.2.13",
  "description": "A port of the EventStore .Net ClientAPI to Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=4.0"
  },
  "engineStrict": true,
  "scripts": {
    "clean": "rm -f lib/dist.js",
    "prebuild": "npm run clean",
    "build": "webpack -o ./lib/dist.js",
    "pretest": "npm run build",
    "test": "nodeunit",
    "test-debug": "TESTS_VERBOSE_LOGGING=1 nodeunit",
    "prepublishOnly": "npm run build && npm run gendocs",
    "gendocs": "rm -rf docs && jsdoc src -r -d docs"
  },
  "files": [
    "lib",
    "docs",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billyfree/node-eventstore-client.git"
  },
  "keywords": [
    "eventstore",
    "geteventstore",
    "node"
  ],
  "author": "Nicolas Dextraze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/billyfree/node-eventstore-client/issues"
  },
  "homepage": "https://github.com/billyfree/node-eventstore-client#readme",
  "dependencies": {
    "long": "^4.0.0",
    "protobufjs": "^6.8.8",
    "strict-event-emitter-types": "^2.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/long": "^4.0.0",
    "@types/node": "^12.7.2",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.3",
    "nodeunit": "^0.11.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
